  },
  "devDependencies": {
    "napa": "3.0.0",
    "tape": "4.10.1"
  },
  "dependencies": {
    "benchmark": "^2.1.4",
    "fast-xml-parser": "^3.12.16",
    "rapidx2j": "^0.5.0",
